            </header>
        </div>
        <!-- Header ends -->

        <!-- background -->
        <div class="small-height-bg">
            <p class="text-center">User Profile</p>
        </div>
        <!-- background -->
        <!-- details conatiner -->
        <div class="general-box">
            <div class="content-box-lg">
                <div class="container">
                    <div class="row">
                        <p class="section-heading">Basic Profile Details</p>
                    </div>
                </div>

                <div class="container">
                    <div class="row">
                        <form>
                            <div class="row">

                                <div class="col-md-6 col-sm-6">
                                    <div class="form-group">
                                        <label class="info-label" for="up-firstname">First Name *</label>
                                        <input type="text" class="form-control form-inbox" id="up-firstname"
                                               aria-describedby="emailHelp" placeholder="Enter your first name">
                                    </div>
                                </div>

                                <div class="col-md-6 col-sm-6">
                                    <div class="form-group">
                                        <label class="info-label" for="up-lastname">Last Name *</label>
                                        <input type="text" class="form-control form-inbox" id="up-lastname"
                                               aria-describedby="emailHelp" placeholder="Enter your last name">
                                    </div>
                                </div>

                                <div class="col-md-6 col-sm-6">
                                    <div class="form-group">
                                        <label class="info-label" for="up-email">Email *</label>
                                        <input type="email" class="form-control form-inbox" id="upemail"
                                               aria-describedby="emailHelp" placeholder="Enter your email address">
                                    </div>
                                </div>

                                <div class="col-md-6 col-sm-6">
                                    <div class="form-group">
                                        <label class="info-label" for="up-dob">Date Of Birth</label>
                                        <input type="text" class="form-control form-inbox" id="up-dob"
                                               aria-describedby="emailHelp" placeholder="Enter your date of birth">
                                        <div class="calendar">
                                            <img class="calendar-img" src="~/Content/User/images/user-profile/calendar.png"
                                                 alt="eye">
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-6 col-sm-6">
                                    <div class="form-group">
                                        <label class="info-label" for="up-gender">Gender</label>
                                        <select class="form-control form-inbox" id="up-gender">
                                            <option selected disabled hidden>Select your gender</option>
                                            <option>Male</option>
                                            <option>Female</option>
                                        </select>
                                        <div class="select-down-arrow">
                                            <img class="select-down-arrow-img"
                                                 src="~/Content/User/images/user-profile/down-arrow.png" alt="eye">
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-2 col-sm-2">
                                    <div class="form-group">
                                        <label class="info-label" for="up-ccode">Code</label>
                                        <select class="form-control form-inbox" id="up-ccode">
                                            <option selected disabled hidden>+91</option>
                                            <option>+91</option>
                                            <option>+1</option>
                                            <option>+53</option>
                                            <option>+11</option>
                                            <option>+43</option>
                                            <option>+83</option>
                                            <option>+25</option>
                                            <option>+12</option>
                                        </select>
                                        <div class="select-down-arrow">
                                            <img class="select-down-arrow-img"
                                                 src="~/Content/User/images/user-profile/down-arrow.png" alt="eye">
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-4 col-sm-4">
                                    <div class="form-group">
                                        <label class="info-label" for="exampleInputEmail1">Phone Number</label>
                                        <input type="text" class="form-control form-inbox" id="exampleInputEmail1"
                                               aria-describedby="emailHelp" placeholder="Enter your phone number">
                                    </div>
                                </div>

                                <div class="col-md-6 col-sm-6">
                                    <div class="form-group">
                                        <label class="info-label" for="uploadProfilePicture">Profile Picture</label>
                                        <div id="uploadProfilePicture" class="form-control form-inbox upload-input-box" onClick="document.getElementById('up-picture').click()" style="cursor: pointer;">
                                            <img src="~/Content/User/images/user-profile/upload.png" alt="Upload Profile Picture">
                                            <p>Upload a picture</p>
                                        </div>
                                        <input type="file" style="display: none;" class="form-control" id="up-picture" name="up-picture">
                                    </div>
                                </div>

                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <!-- details container -->
        <!-- details conatiner -->
        <div class="general-box">
            <div class="content-box-lg">
                <div class="container">
                    <div class="row">
                        <p class="section-heading">Address Details</p>
                    </div>
                </div>

                <div class="container">
                    <div class="row">
                        <form>
                            <div class="row">

                                <div class="col-md-6 col-sm-6">
                                    <div class="form-group">
                                        <label class="info-label" for="up-add1">Address Line 1 *</label>
                                        <input type="text" class="form-control form-inbox" id="up-add1"
                                               aria-describedby="emailHelp" placeholder="Enter your address">
                                    </div>
                                </div>

                                <div class="col-md-6 col-sm-6">
                                    <div class="form-group">
                                        <label class="info-label" for="up-add2">Address Line 2</label>
                                        <input type="text" class="form-control form-inbox" id="up-add2"
                                               aria-describedby="emailHelp" placeholder="Enter your address">
                                    </div>
                                </div>

                                <div class="col-md-6 col-sm-6">
                                    <div class="form-group">
                                        <label class="info-label" for="up-city">City *</label>
                                        <input type="text" class="form-control form-inbox" id="up-city"
                                               aria-describedby="emailHelp" placeholder="Enter your city">
                                    </div>
                                </div>

                                <div class="col-md-6 col-sm-6">
                                    <div class="form-group">
                                        <label class="info-label" for="up-state">State *</label>
                                        <input type="text" class="form-control form-inbox" id="up-state"
                                               aria-describedby="emailHelp" placeholder="Enter your state">
                                    </div>
                                </div>

                                <div class="col-md-6 col-sm-6">
                                    <div class="form-group">
                                        <label class="info-label" for="up-zipcode">ZipCode *</label>
                                        <input type="text" class="form-control form-inbox" id="up-zipcode"
                                               aria-describedby="emailHelp" placeholder="Enter your zipcode">
                                    </div>
                                </div>

                                <div class="col-md-6 col-sm-6">
                                    <div class="form-group">
                                        <label class="info-label" for="up-country">Country *</label>
                                        <select class="form-control form-inbox" id="up-country">
                                            <option value="" disabled selected hidden>Select your country</option>
                                            <option>India</option>
                                            <option>USA</option>
                                            <option>Canada</option>
                                            <option>China</option>
                                            <option>Australia</option>
                                            <option>New Jersy</option>
                                            <option>England</option>
                                        </select>
                                        <div class="select-down-arrow">
                                            <img class="select-down-arrow-img"
                                                 src="~/Content/User/images/user-profile/down-arrow.png" alt="eye">
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <!-- details container -->
        <!-- details conatiner -->
        <div class="general-box">
            <div class="content-box-lg">
                <div class="container">
                    <div class="row">
                        <p class="section-heading">University and College Information</p>
                    </div>
                </div>

                <div class="container">
                    <div class="row">
                        <form>
                            <div class="row">

                                <div class="col-md-6 col-sm-6">
                                    <div class="form-group">
                                        <label class="info-label" for="up-university">University</label>
                                        <input type="text" class="form-control form-inbox" id="up-university"
                                               placeholder="Enter your university">
                                    </div>
                                </div>

                                <div class="col-md-6 col-sm-6">
                                    <div class="form-group">
                                        <label class="info-label" for="up-college">College</label>
                                        <input type="text" class="form-control form-inbox" id="up-college"
                                               placeholder="Enter your college">
                                    </div>
                                </div>

                                <div class="col-md-12 col-sm-12">
                                    <div class="form-btn">
                                        <button type="submit" class="btn btn-general btn-purple">Submit</button>
                                    </div>
                                </div>

                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <!-- details container -->
        <!-- Footer  -->
        <footer>
            <div class="container">
                <div class="row">
                    <div class="col-md-6 col-sm-8 col-12 text-left">
                        <p>Copyright &copy; TatvaSoft All Rights Reserved.</p>
                    </div>
                    <div class="col-md-6 col-sm-4 col-12 text-right">
                        <ul class="social-list">
                            <li><a class="facebook-img" href="#"><img src="~/Content/User/images/Homepage/facebook.png" alt=""></a></li>
                            <li><a class="twitter-img" href="#"><img src="~/Content/User/images/Homepage/twitter.png" alt=""></a></li>
                            <li><a class="linkedin-img" href="#"><img src="~/Content/User/images/Homepage/linkedin.png" alt=""></a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </footer>
        <!-- Footer Ends -->
    </section>


    <!-- JQuery -->
    <script src="~/Scripts/User/js/jquery.js"></script>

    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
